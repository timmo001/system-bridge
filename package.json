{
  "name": "system-bridge",
  "productName": "System Bridge",
  "description": "A bridge for your systems",
  "version": "1.3.1",
  "main": "dist/index.js",
  "types": "lib/",
  "scripts": {
    "build:frontend": "cd frontend && yarn build",
    "compile": "shx rm -rf dist/ && tsc",
    "install:all": "yarn install && cd frontend && yarn install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:junit": "jest --coverage --ci --reporters=default --reporters=jest-junit",
    "make": "yarn compile && yarn build:frontend && cross-env electron-forge make",
    "package": "yarn compile && yarn build:frontend && electron-forge package",
    "publish": "yarn compile && yarn build:frontend && electron-forge publish",
    "start": "concurrently -k \"yarn start:frontend\" \"wait-on tcp:3000 && yarn start:app\"",
    "start:dev": "concurrently -k \"yarn start:frontend\" \"wait-on tcp:3000 && yarn start:app:dev\"",
    "start:prod": "yarn build:frontend && yarn start:app:prod",
    "start:app": "yarn compile && cross-env ELECTRON_IS_DEV=1 electron-forge start",
    "start:app:dev": "nodemon --ext ts,js --watch src --exec \"yarn start:app\"",
    "start:app:prod": "yarn compile && cross-env electron-forge start",
    "start:frontend": "cd frontend && cross-env BROWSER=none yarn start",
    "eslint": "eslint ./src --config .eslintrc.json --ext .ts"
  },
  "keywords": [
    "system",
    "bridge",
    "api",
    "rest",
    "websocket"
  ],
  "author": {
    "name": "Timmo",
    "email": "contact@timmo.xyz",
    "url": "https://system-bridge.timmo.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timmo001/system-bridge"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "asar": true,
        "name": "system-bridge",
        "executableName": "system-bridge",
        "icon": "public/system-bridge-circle.icns",
        "setupIcon": "public/system-bridge-circle.icns",
        "win32metadata": {
          "requested-execution-level": "highestAvailable"
        }
      },
      "makers": [
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "categories": [
                "Utility"
              ],
              "description": "A bridge for your systems",
              "icon": "public/system-bridge-circle.png"
            }
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "icon": "public/system-bridge-circle.icns"
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "options": {
              "categories": [
                "Utility"
              ],
              "description": "A bridge for your systems",
              "icon": "public/system-bridge-circle.png"
            }
          }
        },
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "executableName": "SystemBridge",
            "icon": "public/system-bridge-circle.ico",
            "setupIcon": "public/system-bridge-circle.ico",
            "loadingGif": "public/loading.gif"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "timmo001",
              "name": "system-bridge"
            }
          }
        }
      ]
    }
  },
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.54",
    "@electron-forge/maker-deb": "6.0.0-beta.54",
    "@electron-forge/maker-dmg": "6.0.0-beta.54",
    "@electron-forge/maker-rpm": "6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "6.0.0-beta.54",
    "@electron-forge/maker-zip": "6.0.0-beta.54",
    "@electron-forge/publisher-github": "6.0.0-beta.54",
    "@nestjs/cli": "7.6.0",
    "@nestjs/schematics": "7.3.1",
    "@nestjs/testing": "7.6.15",
    "@types/compression": "1.7.0",
    "@types/cors": "2.8.10",
    "@types/electron-devtools-installer": "2.2.0",
    "@types/express": "4.17.11",
    "@types/helmet": "4.0.0",
    "@types/jest": "26.0.22",
    "@types/mdns": "0.0.33",
    "@types/serve-favicon": "2.5.2",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.0",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "concurrently": "6.0.2",
    "cross-env": "7.0.3",
    "electron": "12.0.2",
    "eslint": "7.24.0",
    "eslint-config-prettier": "8.2.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "shx": "0.3.3",
    "supertest": "6.1.3",
    "ts-jest": "26.5.5",
    "ts-loader": "8.1.0",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.4",
    "wait-on": "5.3.0"
  },
  "dependencies": {
    "@feathersjs/authentication": "4.5.11",
    "@feathersjs/configuration": "4.5.11",
    "@feathersjs/express": "4.5.11",
    "@feathersjs/feathers": "4.5.11",
    "@feathersjs/socketio": "4.5.11",
    "@mdi/js": "5.9.55",
    "@nestjs/common": "7.6.15",
    "@nestjs/core": "7.6.15",
    "@nestjs/mapped-types": "0.4.1",
    "@nestjs/platform-express": "7.6.15",
    "@nestjs/swagger": "4.8.0",
    "axios": "0.21.1",
    "brightness": "3.0.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "electron-debug": "3.2.0",
    "electron-devtools-installer": "3.1.1",
    "electron-settings": "4.0.2",
    "execa": "5.0.0",
    "express": "4.17.1",
    "feathers-swagger": "1.2.1",
    "helmet": "4.4.1",
    "loudness": "0.4.1",
    "music-metadata": "7.8.3",
    "nest-winston": "1.4.2",
    "peer": "0.6.1",
    "query-string": "7.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.7",
    "serve-favicon": "2.5.0",
    "swagger-ui-express": "4.1.6",
    "systeminformation": "5.6.12",
    "update-electron-app": "2.0.1",
    "uuid": "8.3.2",
    "winston": "3.3.3"
  },
  "optionalDependencies": {
    "mdns": "2.7.2"
  }
}
