{
  "name": "system-bridge",
  "description": "System Bridge",
  "version": "1.0.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Aidan Timson",
    "email": "contact@timmo.xyz"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^15.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "yarn run lint && yarn run compile && yarn run jest",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "start:dev": "ts-node-dev --no-notify src/",
    "start": "yarn run compile && node lib/",
    "jest": "jest  --forceExit",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
