{
  "name": "system-bridge",
  "productName": "system-bridge",
  "version": "1.0.0",
  "description": "System Bridge",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "ts-node-dev --no-notify ./src",
    "start": "yarn compile && electron-forge start",
    "compile": "shx rm -rf dist/ && tsc",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Aidan Timson",
    "email": "contact@timmo.xyz"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ts_only_electron"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.54",
    "@electron-forge/maker-deb": "6.0.0-beta.54",
    "@electron-forge/maker-rpm": "6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "6.0.0-beta.54",
    "@electron-forge/maker-zip": "6.0.0-beta.54",
    "@types/electron-devtools-installer": "2.2.0",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "concurrently": "5.3.0",
    "cross-env": "7.0.3",
    "electron": "11.2.3",
    "electron-devtools-installer": "3.1.1",
    "eslint": "7.20.0",
    "eslint-plugin-import": "2.20.0",
    "shx": "0.3.3",
    "ts-node-dev": "1.1.1",
    "typescript": "4.0.2",
    "wait-on": "5.2.1"
  },
  "dependencies": {
    "@feathersjs/express": "4.5.11",
    "@feathersjs/feathers": "4.5.11",
    "@feathersjs/socketio": "4.5.11",
    "@mdi/js": "5.9.55",
    "electron-settings": "4.0.2",
    "electron-squirrel-startup": "1.0.0",
    "execa": "5.0.0",
    "express": "4.17.1",
    "feathers-swagger": "1.2.1",
    "systeminformation": "5.1.2"
  }
}
