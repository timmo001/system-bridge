{
  "name": "system-bridge",
  "version": "2.8.1",
  "description": "A bridge for your systems",
  "private": true,
  "author": {
    "name": "Aidan Timson <Timmo>",
    "email": "contact@timmo.xyz",
    "url": "https://system-bridge.timmo.dev"
  },
  "keywords": [
    "system",
    "bridge"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/timmo001/system-bridge"
  },
  "license": "MIT",
  "bin": "dist/index.js",
  "scripts": {
    "build": "yarn build:frontend && yarn build:app && yarn build:gui",
    "build:app": "rimraf dist && nest build",
    "build:frontend": "cd frontend && yarn build",
    "build:gui": "cd gui && pip3 install . && pyinstaller --noconfirm system-bridge-gui.spec",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "install:all": "yarn install && cd frontend && yarn install",
    "install:all:ci": "yarn install --pure-lockfile && cd frontend && yarn install --pure-lockfile",
    "upgrade:all": "yarn upgrade --latest --exact && cd frontend && yarn upgrade --latest --exact",
    "upgrade-interactive:all": "yarn upgrade-interactive --latest --exact && cd frontend && yarn upgrade-interactive --latest --exact",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "package": "rimraf out && yarn build && node scripts/package.js",
    "package:app": "rimraf out && yarn build:app && cross-env APP_ONLY=true node scripts/package.js",
    "start": "concurrently -k \"yarn start:app\" \"yarn start:frontend\"",
    "start:dev": "concurrently -k \"yarn start:app:dev\" \"yarn start:frontend:dev\"",
    "start:dev:cli": "concurrently -k \"yarn start:app:cli:dev\" \"yarn start:frontend:dev\"",
    "start:dev:nogui": "concurrently -k \"yarn start:app:nogui:dev\" \"yarn start:frontend:dev\"",
    "start:prod": "cross-env SB_PACKAGED=false node dist/",
    "start:prod:cli": "cross-env SB_PACKAGED=false SB_CLI=true SB_GUI=false node dist/",
    "start:app": "cross-env NODE_ENV=development SB_PACKAGED=false nest start",
    "start:app:cli": "cross-env NODE_ENV=development SB_CLI=true SB_PACKAGED=false SB_GUI=false nest start",
    "start:app:cli:dev": "cross-env NODE_ENV=development SB_CLI=true SB_PACKAGED=false SB_GUI=false nest start --watch",
    "start:app:nogui:dev": "cross-env NODE_ENV=development SB_PACKAGED=false SB_GUI=false nest start --watch",
    "start:app:debug": "cross-env NODE_ENV=development SB_PACKAGED=false nest start --debug --watch",
    "start:app:dev": "cross-env NODE_ENV=development SB_PACKAGED=false nest start --watch",
    "start:cli": "yarn start:app:cli",
    "start:cli:dev": "concurrently -k \"yarn start:app:cli:dev\"",
    "start:frontend": "cd frontend && yarn start",
    "start:frontend:dev": "cd frontend && yarn start:dev",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:junit": "jest --coverage --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@nestjs/common": "8.2.4",
    "@nestjs/core": "8.2.4",
    "@nestjs/platform-express": "8.2.4",
    "@nestjs/platform-ws": "8.2.4",
    "@nestjs/swagger": "5.1.5",
    "@nestjs/typeorm": "8.0.2",
    "@nestjs/websockets": "8.2.4",
    "auto-launch": "5.0.5",
    "axios": "0.24.0",
    "better-sqlite3": "7.4.5",
    "dotenv": "10.0.0",
    "execa": "5.1.1",
    "helmet": "4.6.0",
    "loudness": "0.4.1",
    "mime-types": "2.1.34",
    "mqtt": "4.2.8",
    "next": "11.1.3",
    "node-notifier": "10.0.0",
    "open": "8.4.0",
    "peer": "0.6.1",
    "platform-folders": "0.5.4",
    "query-string": "7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0",
    "semver": "7.3.5",
    "swagger-ui-express": "4.3.0",
    "system-bridge-windows-sensors": "1.2.5",
    "systeminformation": "5.9.17",
    "toad-scheduler": "1.6.0",
    "typeorm": "0.2.41",
    "uuid": "8.3.2",
    "winston": "3.3.3",
    "ws": "7.5.6"
  },
  "devDependencies": {
    "@nestjs/cli": "8.1.6",
    "@nestjs/schematics": "8.0.5",
    "@nestjs/testing": "8.2.4",
    "@types/auto-launch": "5.0.2",
    "@types/express": "4.17.13",
    "@types/helmet": "4.0.0",
    "@types/jest": "27.0.3",
    "@types/mdns": "0.0.34",
    "@types/mime-types": "2.1.1",
    "@types/node": "17.0.4",
    "@types/node-notifier": "8.0.1",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/semver": "7.3.9",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.3",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "concurrently": "6.5.1",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "fs-extra": "10.0.0",
    "jest": "27.4.5",
    "jest-junit": "13.0.0",
    "pkg": "5.5.1",
    "prettier": "2.5.1",
    "shx": "0.3.3",
    "supertest": "6.1.6",
    "ts-jest": "27.1.1",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.4"
  },
  "optionalDependencies": {
    "mdns": "2.7.2",
    "node-hide-console-window": "2.1.0",
    "robotjs": "0.6.0",
    "win-audio": "2.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "node"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "pkg": {
    "assets": [
      "frontend/.next/**/*",
      "frontend/node_modules/**/*",
      "node_modules/**/*",
      "public/**/*"
    ],
    "outputPath": "out",
    "scripts": [
      "dist/**/**.js",
      "dist/index.js",
      "dist/components/api/index.js",
      "dist/components/api/audio/data/win32.js",
      "dist/components/api/audio/data/unix.js",
      "frontend/.next/**/*.js"
    ],
    "targets": [
      "host"
    ]
  }
}
