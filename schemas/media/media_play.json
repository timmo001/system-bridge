{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "Media Play",
  "properties": {
    "album": {
      "type": "string"
    },
    "artist": {
      "type": "string"
    },
    "autoplay": {
      "type": "boolean",
      "default": false
    },
    "cover": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "volume": {
      "type": "number",
      "default": 40
    }
  },
  "required": ["url"]
}
