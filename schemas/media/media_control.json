{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "Media Control",
  "properties": {
    "action": {
      "type": "string",
      "enum": [
        "play",
        "pause",
        "stop",
        "previous",
        "next",
        "seek",
        "rewind",
        "fastforward",
        "shuffle",
        "repeat",
        "mute",
        "volumedown",
        "volumeup"
      ]
    },
    "value": {}
  },
  "required": ["action"]
}
