{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Main",
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "preLaunchTask": "Python: pip install",
      "program": "gui/__init__.py",
      "args": [
        "--host",
