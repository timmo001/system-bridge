{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Node.js: yarn start",
      "command": "yarn start",
      "request": "launch",
      "type": "node-terminal",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Main",
      "type": "python",
      "args": [
        "--host",
        "localhost",
        "--api-key",
        "${env:SB_API_KEY}",
        "--api-port",
        "9170",
        "--websocket-port",
        "9172",
        "--log-level",
        "debug"
      ],
      "request": "launch",
      "program": "gui/main.py",
      "console": "integratedTerminal"
    }
  ]
}
