{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "args": [
        "--host",
        "localhost",
        "--api-key",
        "${env:SB_API_KEY}",
        "--api-port",
        "9170",
        "--websocket-port",
        "9172",
        "--log-level",
        "debug"
      ],
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    }
  ]
}
